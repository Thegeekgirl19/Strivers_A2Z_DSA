package com.ezdi.ocr.dto.common;

public class Point
{
	private double	X;
	private double	Y;

	public Point(double x, double y)
	{
		this.X = x;
		this.Y = y;
	}

	public double getX()
	{
		return X;
	}

	public double getY()
	{
		return Y;
	}

	@Override
	public String toString()
	{
		StringBuilder builder = new StringBuilder();
		builder.append(X);
		builder.append("\t");
		builder.append(Y);
		return builder.toString();
	}

}