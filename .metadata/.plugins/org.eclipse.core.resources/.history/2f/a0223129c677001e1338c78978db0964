package com.ezdi.ocr.dto.pdftotext;

import java.util.List;

import com.ezdi.ocr.dto.common.Line;

public class DualIdentifiedPage
{
	private int			index;
	private List<Line>	left;
	private List<Line>	right;
	private List<Line>	other;

	public DualIdentifiedPage(int index, List<Line> left, List<Line> right, List<Line> other)
	{
		this.index = index;
		this.left = left;
		this.right = right;
		this.other = other;
	}

	public int getIndex()
	{
		return index;
	}

	public List<Line> getLeft()
	{
		return left;
	}

	public List<Line> getRight()
	{
		return right;
	}

	public List<Line> getOther()
	{
		return other;
	}
}
