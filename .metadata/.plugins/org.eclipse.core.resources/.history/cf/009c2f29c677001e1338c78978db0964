package com.ezdi.ocr.dto.common;

import java.util.List;

public class DualIdentifiedPage
{
	private int			index;
	private List<Block>	left;
	private List<Block>	right;
	private List<Block>	other;
	private List<Block>	header;
	private List<Block>	footer;

	public DualIdentifiedPage(int index, List<Block> header, List<Block> left, List<Block> right, List<Block> other, List<Block> footer)
	{
		this.index = index;
		this.header = header;
		this.left = left;
		this.right = right;
		this.other = other;
		this.footer = footer;
	}

	public int getIndex()
	{
		return index;
	}

	public List<Block> getHeader()
	{
		return header;
	}

	public List<Block> getLeft()
	{
		return left;
	}

	public List<Block> getRight()
	{
		return right;
	}

	public List<Block> getOther()
	{
		return other;
	}

	public List<Block> getFooter()
	{
		return footer;
	}

}
